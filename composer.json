{
  "name": "pbweb/mimic",
  "description": "PBWeb Mimic",
  "keywords": ["pbweb", "mimic", "testing", "mock"],
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "http://satis.pbwdev.com"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "sebastian/phpcpd": "~2.0",
    "phploc/phploc": "~2.0",
    "mayflower/php-codebrowser": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\Mimic\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
