{
  "name": "pbweb/mimic",
  "description": "PBWeb Mimic",
  "keywords": ["pbweb", "mimic", "testing", "mock"],
  "license": "proprietary",
  "require": {
    "php": "^5.5|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.3",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^2.1",
    "nicofuma/php-codebrowser": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\Mimic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pbweb\\Mimic\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
