{
  "name": "pbweb/mimic",
  "description": "Mimic classes for functional testing",
  "keywords": ["mimic", "class", "library", "testing", "mock", "stub"],
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^2.8",
    "pbweb/pbweb-coding-standard": "^1.1.1"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\Mimic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pbweb\\Mimic\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
